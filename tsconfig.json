{
  "compilerOptions": {
    // "target": "es5",
    "target": "ES2024",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@*": [
        "./src/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/features/*": [
        "src/features/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/types/*": [
        "src/types/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/layout.tsx",
    "src/app/dashboard/page.tsx",
    "src/app/dashboard/DashboardClient.tsx",
    "src/app/patients/page.tsx",
    "src/app/patients/create/page.tsx",
    "src/app/patients/edit/[id]/page.tsx",
    "src/app/patients/show/[id]/page.tsx",
    "src/app/page.tsx",
    "src/app/dashboard/index.tsx",
    "src/app/login/page.tsx",
    "src/app/patients/show.tsx",
    "src/app/practitioners/page.tsx",
    "src/app/practitioners/create/page.tsx",
    "src/app/practitioners/edit/[id]/page.tsx",
    "src/app/practitioners/show/[id]/page.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}